  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
